<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.192" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <GridPane prefHeight="800.0" prefWidth="800.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="800.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="700.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="800.0" spacing="10.0">
            <TextField fx:id="_textField_name" text="Task" />
            <TextField fx:id="_textField_value" text="Value" />
            <TextField fx:id="_textField_duration" text="Duration" />
         <Button fx:id="_button_addTask" mnemonicParsing="false" text="Add" />
        </HBox>
        <VBox fx:id="_tableView_tasks" GridPane.rowIndex="1" />
    </GridPane>
    <HBox alignment="CENTER_RIGHT" spacing="10.0" GridPane.rowIndex="1">
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
        <Button mnemonicParsing="false" text="Calculate Efficiency" />
    </HBox>
</GridPane>
